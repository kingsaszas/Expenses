<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@MainWindowStyle.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
   <children>
      <Pane fx:id="titlePane" layoutY="1.0" prefHeight="25.0" prefWidth="600.0">
