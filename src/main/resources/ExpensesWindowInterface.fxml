<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="root" stylesheets="@ExpensesWindowStyle.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ExpensesWindowController">
   <children>
      <Pane fx:id="titlePane" layoutX="5.0" layoutY="1.0" prefHeight="25.0" prefWidth="600.0">
         <children>
            <Label layoutX="54.0" layoutY="3.0" prefHeight="17.0" prefWidth="134.0" text="Expenses Calculator" textFill="#f7e4e4">
               <font>
                  <Font size="13.0" />
               </font>
            </Label>
            <ImageView fx:id="btnHide" fitHeight="19.0" fitWidth="22.0" layoutX="517.0" layoutY="3.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../images/hide.png" />
               </image>
            </ImageView>
            <ImageView fx:id="btnClose" fitHeight="26.0" fitWidth="19.0" layoutX="549.0" layoutY="3.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../images/close.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="20.0" fitWidth="16.0" layoutX="34.0" layoutY="4.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../images/icon.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
      <Label fx:id="yearLbl" layoutX="93.0" layoutY="40.0" prefHeight="18.0" prefWidth="75.0" text="Year" textFill="#faf7f7">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label fx:id="monthLbl" layoutX="18.0" layoutY="40.0" prefHeight="18.0" prefWidth="75.0" text="Month" textFill="#faf7f7">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <StackPane layoutX="14.0" layoutY="58.0" prefHeight="326.0" prefWidth="575.0">
         <children>
            <TableView fx:id="expensesTable" prefHeight="162.0" prefWidth="208.0" styleClass="mainPane">
              <columns>
                <TableColumn fx:id="amountColumn" maxWidth="80.0" prefWidth="75.0" text="Amount" />
                <TableColumn fx:id="descriptionColumn" maxWidth="495.0" minWidth="75.0" prefWidth="495.0" resizable="false" text="Description" />
              </columns>
            </TableView>
         </children>
      </StackPane>
   </children>
</AnchorPane>
